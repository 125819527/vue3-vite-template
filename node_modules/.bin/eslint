#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/moanac/work/source/vue3-admin/vue3-vite-template/node_modules/.pnpm/eslint@6.7.2/node_modules/eslint/bin/node_modules:/Users/moanac/work/source/vue3-admin/vue3-vite-template/node_modules/.pnpm/eslint@6.7.2/node_modules/eslint/node_modules:/Users/moanac/work/source/vue3-admin/vue3-vite-template/node_modules/.pnpm/eslint@6.7.2/node_modules:/Users/moanac/work/source/vue3-admin/vue3-vite-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/moanac/work/source/vue3-admin/vue3-vite-template/node_modules/.pnpm/eslint@6.7.2/node_modules/eslint/bin/node_modules:/Users/moanac/work/source/vue3-admin/vue3-vite-template/node_modules/.pnpm/eslint@6.7.2/node_modules/eslint/node_modules:/Users/moanac/work/source/vue3-admin/vue3-vite-template/node_modules/.pnpm/eslint@6.7.2/node_modules:/Users/moanac/work/source/vue3-admin/vue3-vite-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../eslint/bin/eslint.js" "$@"
fi
